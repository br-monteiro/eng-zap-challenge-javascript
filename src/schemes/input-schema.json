{
  "type": "object",
  "properties": {
    "usableAreas": {
      "type": "integer",
      "minimum": 0
    },
    "listingType": {
      "type": "string",
      "minLength": 3
    },
    "createdAt": {
      "type": "string",
      "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}[A-Z][0-9]{2}:[0-9]{2}:[0-9]{2}(.[0-9]{3})?[A-Z]"
    },
    "updatedAt": {
      "type": "string",
      "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}[A-Z][0-9]{2}:[0-9]{2}:[0-9]{2}(.[0-9]{3})?[A-Z]"
    },
    "listingStatus": {
      "type": "string",
      "minLength": 3
    },
    "id": {
      "type": "string",
      "minLength": 3
    },
    "parkingSpaces": {
      "type": "integer",
      "minimum": 0
    },
    "bathrooms": {
      "type": "integer",
      "minimum": 0
    },
    "bedrooms": {
      "type": "integer",
      "minimum": 0
    },
    "owner": {
      "type": "boolean"
    },
    "images": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string",
        "pattern": "^https?:\\/{2}.+$"
      }
    },
    "address": {
      "type": "object",
      "properties": {
        "city": {
          "type": "string"
        },
        "neighborhood": {
          "type": "string"
        },
        "geoLocation": {
          "type": "object",
          "properties": {
            "precision": {
              "type": "string",
              "minLength": 3
            },
            "location": {
              "type": "object",
              "properties": {
                "lon": {
                  "type": "number"
                },
                "lat": {
                  "type": "number"
                }
              }
            }
          }
        }
      }
    },
    "pricingInfos": {
      "type": "object",
      "properties": {
        "yearlyIptu": {
          "type": "string",
          "minLength": 1
        },
        "price": {
          "type": "string",
          "minLength": 1
        },
        "businessType": {
          "type": "string",
          "minLength": 3
        },
        "monthlyCondoFee": {
          "type": "string",
          "minLength": 1
        }
      }
    }
  }
}
